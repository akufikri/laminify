{
    "name": "akufikri/laminify",
    "description": "A Laravel package for JS and CSS minification",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Akufikri\\Laminify\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akufikri\\Laminify\\LaminifyServiceProvider"
            ],
            "aliases": {
                "Laminify": "Akufikri\\Laminify\\Facades\\Laminify"
            }
        }
    },
    "suggest": {
        "blade-components": "This package provides Blade components for wrapping and minifying JS and CSS."
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Akufikri",
            "email": "codingwithfikri@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
