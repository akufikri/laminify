{
    "name": "vendor/laminify",
    "description": "A Laravel package for JS and CSS minification",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Laminify\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\Laminify\\LaminifyServiceProvider"
            ],
            "aliases": {
                "Laminify": "Vendor\\Laminify\\Facades\\Laminify"
            }
        }
    },
    "suggest": {
        "blade-components": "This package provides Blade components for wrapping and minifying JS and CSS."
    }
}